{
  "name": "company-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --no-lint",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint --max-warnings=0 --no-cache",
    "unit:test": "jest unit",
    "integration:test": "npm run startmb & jest integration && mb stop",
    "startmb": "mb --configfile ./mountebank/exampleImposters.json --loglevel debug",
    "stopmb": "mb stop",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "db:setup": "prisma db push && prisma generate && prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.7",
    "@total-typescript/ts-reset": "github:total-typescript/ts-reset",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.2",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.4"
  }
}
